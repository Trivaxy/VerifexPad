# Server configuration
PORT=3001
NODE_ENV=development

# Compiler bootstrap configuration
VERIFEX_VERSION=master
VERIFEX_COMPILER_REPO=https://github.com/Trivaxy/Verifex.git
Z3_DOWNLOAD_URL=https://github.com/Z3Prover/z3/releases/download/z3-4.12.2/z3-4.12.2-x64-glibc-2.31.zip

# Podman configuration for code execution
PODMAN_PATH=podman
PODMAN_IMAGE=mcr.microsoft.com/dotnet/runtime:9.0
PODMAN_EXTRA_ARGS=
PODMAN_MEMORY_LIMIT=512m
PODMAN_PIDS_LIMIT=64
SANDBOX_TIMEOUT_MS=10000
SANDBOX_DISABLED=false
